---
interface Props {
  bgcolor: string;
  imagealt: string;
  image: string;
}

const { bgcolor,imagealt, image } = Astro.props;

---
<div class="flex flex-col  items-center pt-xxs pb-s ">
<div class={`p-6 w-4xl h-10xl rounded-full flex justify-center items-center ${bgcolor}`}>
  <img class="h-8xl " src={image} alt={imagealt} />
</div>

</div>